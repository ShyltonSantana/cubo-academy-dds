const numeros = [1, 2, 3, 4, 5]
const nomes = ["joao", "maria", "jose", "pedro", "paula"];
console.log(nomes);
console.log(nomes[0], numeros[0]);
console.log(nomes[1], numeros[1]);
console.log(nomes[2], numeros[2]);
console.log(nomes[3], numeros[3]);
console.log(nomes[4], numeros[4]);
console.log(nomes[5], numeros[5]);